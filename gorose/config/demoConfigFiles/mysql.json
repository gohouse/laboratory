{
  "Driver": "mysql",
  "Default": "mysql",
  "QueryLog": false,
  "SetMaxOpenConns": 0,
  "SetMaxIdleConns": 0,
  "Dsns": {
    "mysql": "username:password@tcp(localhost:3306)/database?charset=utf8",
    "sqlite": "~/path/to/foo.db",
    "postgres": "host=localhost port=5432 user=username password=password dbname=databases sslmode=disable",
    "oracle": "username/password@database",
    "mssql": "server=localhost;port=1433;database=databases;user id=username;password=password"
  }
}